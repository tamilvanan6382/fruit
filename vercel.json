{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/send-whatsapp",
      "dest": "/api/send-whatsapp.js"
    },
    {
      "src": "/api/send-telegram",
      "dest": "/api/send-telegram.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}